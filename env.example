# EUDR API Configuration
# These are the required credentials for accessing the EUDR system

# Your EUDR username (provided by EUDR system administrators)
EUDR_TRACES_USERNAME=your_username_here

# Your EUDR password (provided by EUDR system administrators)
EUDR_TRACES_PASSWORD=your_password_here

# Base URL for EUDR API endpoints
# Use acceptance environment for testing, production for live use https://eudr.webcloud.ec.europa.eu/tracesnt/%20
EUDR_TRACES_BASE_URL=https://acceptance.eudr.webcloud.ec.europa.eu

# Web service client identifier
EUDR_WEB_SERVICE_CLIENT_ID=eudr-test

# API timeout in milliseconds (default: 30000 = 30 seconds)
EUDR_TRACES_TIMEOUT=30000

# Web client ID (usually 'acceptance' for testing)
EUDR_WEB_CLIENT_ID=acceptance

# Test Configuration
# Set to 'test' for test environment
NODE_ENV=test

# Integration Test Configuration (Optional)
# These are used for testing with real data references

# Test DDS UUID for retrieval tests (optional)
# This should be a real DDS identifier from your system
TEST_DDS_UUID=your_test_dds_uuid_here

# Test reference number for retrieval tests (optional)
# This should be a real reference number from your system
TEST_REFERENCE_NUMBER=your_test_reference_number_here

# Test verification number for retrieval tests (optional)
# This should be a real verification number from your system
TEST_VERIFICATION_NUMBER=your_test_verification_number_here

# Additional Configuration (Optional)
# These settings can be customized based on your needs

# Timestamp validity in seconds (default: 60)
# EUDR_TIMESTAMP_VALIDITY=60

# Debug mode for detailed logging (set to 'true' to enable)
# DEBUG=false

# Log level for tests (error, warn, info, debug)
# LOG_LEVEL=info

# EUDR API Client Logging Configuration
# Set the logging level for the EUDR API Client
# Valid levels: trace, debug, info, warn, error, fatal
# Default: warn (reduces noise during testing)
EUDR_LOG_LEVEL=warn
